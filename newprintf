#include "main.h"

/**
*print_string - print string to stdout
*@garg: argument
*Return: printed string
*/

int_print string(va_list arg)
{
       char *str;
       unsigned int 1;

       str = vs_arg(arg, char *);

       for (1 = 0; str(1); 1++)
       {
              _putchar(str(1));
       }

       return (1);
}
